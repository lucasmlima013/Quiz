<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Guia Perguntas</title>
    <link rel="stylesheet" href="/css/style.css">
    <link rel="stylesheet" href="/css/bootstrap.min.css">
</head>
<body>

    <img src="/img/queima.pmg">

    <h1>Guia de Perguntas 1</h1>
    <hr>
    <h3>Lista de Perguntas</h3>

    Nome: <%= nome %> 
    <hr>
    Linguagem: <%= lang %>
    <hr>
    Empresa: <%= empresa %>
    <hr>
    Qde_Incristos: <%= inscritos %>
    <hr>


    <% if(msg == true){ %>
        <h3>Isso Ã© uma mensagem de erro!</h3>
    <% } else { %>
        <h3>Nenhum Erro!</h3>
    <% } %>


    <% produtos.forEach(function(produto) { %>
            <hr>
            <h4><%= produto.nome %></h4>
            <h5>Preco: R$ <%= produto.preco %></h5>
        <% }) %>

</body>

